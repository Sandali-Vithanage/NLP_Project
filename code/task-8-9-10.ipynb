   "source": [
    "from sentence_transformers import SentenceTransformer\n",
    "from sklearn.metrics.pairwise import cosine_similarity\n",
    "import os\n",
    "import numpy as np\n",
    "\n",
    "model = SentenceTransformer('paraphrase-MiniLM-L6-v2')"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "34c18093",
   "metadata": {},
   "source": [
    "### Compute Cosine Similarity between the embeddings for each label-text pair. Find label-text pair with low semantic similarities."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "b6ea319e",
   "metadata": {},
   "outputs": [],
   "source": [
    "def compute_semantic_similarity(event_folder_path, threshold=0.5):\n",
    "    image_folder = os.path.join(event_folder_path, \"images\")\n",
    "    text_folder = os.path.join(event_folder_path, \"text\")\n",
    "    low_similarity_pairs = []\n",
    "\n",
    "    for image_file in os.listdir(image_folder):\n",
    "        if image_file.endswith(\".jpg\"):\n",
    "            text_file_path = os.path.join(text_folder, image_file.replace(\".jpg\", \".txt\"))\n",
    "            if not os.path.exists(text_file_path):\n",
    "                continue\n",
    "\n",
    "            with open(text_file_path, 'r') as f:\n",
    "                text_content = f.read()\n",
    "                text_labels = [word for word in text_content.split() if word.startswith(\"#\")]\n",
    "\n",
    "            # encoding text content and labels\n",
    "            text_embedding = model.encode(text_content, convert_to_tensor=True)\n",
    "            label_embeddings = model.encode(text_labels, convert_to_tensor=True)\n",
    "\n",
    "            # calculating cosine similarity for each label-text pair\n",
    "            for i, label in enumerate(text_labels):\n",
    "                similarity_score = cosine_similarity([label_embeddings[i].cpu().numpy()], [text_embedding.cpu().numpy()])[0][0]\n",
    "                \n",
    "                # in case if similarity is below the threshold adding to low_similarity_pairs\n",
    "                if similarity_score < threshold:\n",
    "                    low_similarity_pairs.append((label, text_content, similarity_score))\n",
    "\n",
    "    return low_similarity_pairs"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "caca2dc2",
   "metadata": {},
   "outputs": [],
   "source": [
    "# using a dictionary to store results\n",
    "low_similarity_results = {}\n",
    "\n",
    "# processing each event folder\n",
    "for event in event_folders:\n",
    "    event_path = os.path.join(base_path, event)\n",
    "    low_similarity_pairs = compute_semantic_similarity(event_path, threshold=0.5)\n",
    "    low_similarity_results[event] = low_similarity_pairs"
   ]
